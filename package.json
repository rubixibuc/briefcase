{
  "name": "@rubixibuc/briefcase",
  "version": "0.0.4",
  "description": "Visually hidden case formatting",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib --ignore \"src/**/test.js\"",
    "test": "mocha --require @babel/register \"src/**/*test.js\"",
    "clean": "git clean -dxf",
    "lint": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepublishOnly": "npm run clean && npm ci && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rubixibuc/briefcase.git"
  },
  "keywords": [
    "brief",
    "case",
    "briefcase",
    "formatting",
    "caseing"
  ],
  "author": "rubixibuc",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2"
  },
  "files": [
    "lib"
  ]
}
