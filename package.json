{
  "name": "@rubixibuc/briefcase",
  "version": "0.9.1",
  "description": "Visually hidden case formatting",
  "main": "lib/index.js",
  "scripts": {
    "prettier": "prettier --write \"src/**/*.{js,json,md}\" ./README.md",
    "clean": "git clean -dxf",
    "test": "nyc --reporter=html --reporter=text mocha --require @babel/register \"src/**/*test.js\"",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "babel src -d lib --ignore \"src/**/test.js\"",
    "prepublishOnly": "npm run clean && npm ci && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rubixibuc/briefcase.git"
  },
  "keywords": [
    "brief",
    "case",
    "briefcase",
    "formatting",
    "encoding",
    "decoding",
    "non",
    "printable",
    "non-printable"
  ],
  "author": "rubixibuc",
  "license": "Unlicense",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2"
  },
  "files": [
    "lib"
  ]
}
